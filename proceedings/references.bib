@incollection{real-time-computing,
	abstract = {We are entering into a ``big data'' era. Due to bottlenecks---such as poor scalability, difficulties with installation and maintenance, fault tolerance, and low performance in a traditional information technology frameworks---we need to leverage Cloud computing techniques and solutions to deal with big data problems. Cloud computing and big data are complementary to each other and have inherent dialectical unity. The breakthrough of big data technologies will not only resolve the aforementioned problems, but will also promote the wide application of Cloud computing and ``Internet of Things'' technologies. In this chapter, we focus on the development and pivotal technologies of big data, providing a comprehensive description of big data from several perspectives---including the development of big data, the current data-burst situation, the relationship between big data and Cloud computing, big data technologies and related research.},
	address = {Boston},
	author = {Wenhong Tian and Yong Zhao},
